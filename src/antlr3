antlr3a=/usr/share/java
export CLASSPATH=.:$antlr3a/stringtemplate.jar:$antlr3a/antlr3.jar:$antlr3a/antlr3-runtime.jar:$antlr3/antlr-2.7.7.jar
if test -z "$1"
then
  echo CS USAGE: antlr3 t.g, then antlr3 *.java, then antlr3 Main
  java org.antlr.Tool "$@"
else case "$1" in
  *.java)
    javac "$@"
    ;;
  *.g)
    java org.antlr.Tool "$@"
    ;;
  *)
    java "$@"
    ;;
esac fi
